n flag then add the class\n            flag-icon-squared as well. -->\n          </CCol>\n          <template v-for=\"(flag, flagName) in displayedFlags\">\n            <CCol\n              class=\"mb-5\"\n              col=\"3\"\n              sm=\"2\"\n              :key=\"flagName\"\n            >\n              <CIcon :height=\"42\" :content=\"flag\"/>\n              <div>{{toKebabCase(flagName)}}</div>\n            </CCol>\n          </template>\n        </CRow>\n      </CCardBody>\n    </CCard>\n  </div>\n</template>\n\n<script>\nimport { flagSet } from '@coreui/icons'\nexport default {\n  name: 'Flags',\n  flagSet,\n  computed: {\n    displayedFlags () {\n      return this.$options.flagSet\n    }\n  },\n  methods: {\n    toKebabCase (str) {\n      return str.replace(/([a-z])([A-Z0-9])/g, '$1-$2').toLowerCase()\n    }\n  }\n}\n</script>\n"],"sourceRoot":"src/views/icons"}]}