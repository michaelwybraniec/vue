exports[`CDropdown renders correctly inNav 1`] = `
<li
  class="dropleft nav-item"
>
  <a
    aria-expanded="false"
    aria-haspopup="true"
    class="additional-toggler-class nav-link bg-success"
    href="#"
    target="_self"
  >
    Dropdown button
  </a>
   
  <!---->
   
  <div
    class="additional-menu-class dropdown-menu"
  >
